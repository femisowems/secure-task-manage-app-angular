{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/browser",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://secure-task-manage-backend-api-production.up.railway.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}