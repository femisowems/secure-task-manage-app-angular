{
  "name": "dashboard",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "prestart": "npx tailwindcss -i src/styles.tailwind.css -o src/styles.css --minify",
    "start": "ng serve",
    "start:prod": "ng serve --configuration production",
    "start:dev": "ng serve --configuration development",
    "prebuild": "npx tailwindcss -i src/styles.tailwind.css -o src/styles.css --minify",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "packageManager": "npm@10.8.2",
  "dependencies": {
    "@angular/animations": "^19.2.18",
    "@angular/cdk": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.2.18",
    "@angular/router": "^19.1.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tailwindcss/postcss": "^4.1.18",
    "autoprefixer": "^10.4.24",
    "lucide-angular": "^0.564.0",
    "postcss": "^8.5.6",
    "rxjs": "~7.8.0",
    "tailwindcss": "^4.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^19.1.0",
    "@angular/cli": "^19.1.0",
    "@angular/compiler-cli": "^19.1.0",
    "@tailwindcss/cli": "^4.1.18",
    "@testing-library/angular": "^17.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jest-preset-angular": "^16.0.0",
    "jsdom": "^27.1.0",
    "typescript": "~5.8.3",
    "vitest": "^4.0.8"
  }
}